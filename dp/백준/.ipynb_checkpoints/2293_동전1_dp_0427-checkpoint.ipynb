   "source": [
    "\n",
    "# m원이 되는 경우의 수 구하기\n",
    "# dp[i] = dp[i-k]+1, k원\n",
    "n, m = map(int, input().split())\n",
    "arr = []\n",
    "for _ in range(n):\n",
    "    arr.append(int(input()))\n",
    "dp = [0]*(m+1)\n",
    "dp[0] =1 # dp[0]부터 보텀업으로 경우의 수를 구함 -> 스타트 지점 1로 초기화\n",
    "for i in arr:\n",
    "    for j in range(i, m+1):\n",
    "        dp[j] += dp[j-i] \n",
    "print(dp[m])\n"
   ]
  }
