    "n, k = map(int,input().split())\n",
    "\n",
    "m = 1\n",
    "cnt = 0\n",
    "flag = False\n",
    "\n",
    "while m <= n:\n",
    "    if n % m == 0:\n",
    "        cnt += 1\n",
    "    if cnt == k:\n",
    "        flag = True\n",
    "        break\n",
    "    m += 1\n",
    "    \n",
    "if flag ==True:\n",
    "    print(m)\n",
    "else:\n",
    "    print(0)\n",
    "    "
